pipeline {
agent any 

  stages {  
	
    stage ('TEST') {
		steps  {
		sh 'gcloud config list'
		sh 'gcloud compute instances list'
		
		}
		}


stage ('vm-create')  {
steps {
 sh 'gcloud compute instances create vbn --zone=asia-east2-a'
 }
 }

stage ('vpc-create') {
steps {
sh 'gcloud compute networks create klm'
}
}
 
 stage ('list vpc') {
 steps {
 sh 'gcloud compute networks list'
 }
 }
 stage ('list-vm') {
 steps {
 sh 'gcloud compute instances list'
 }
 }
 
}
}
   
